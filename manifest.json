{
	"manifest_version": 3,

	"name": "Gender equalizer",
	"description": "Revolutionize your browsing experience",
	"version": "1.0.1",
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["replacer.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}
